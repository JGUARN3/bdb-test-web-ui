{
  "name": "bdb-test-web-ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run assets && ng serve",
    "build": "npm run assets && ng build",
    "assets": "export ENV=LOCAL && node replace-assets-url.js",
    "assets:dev": "export ENV=DEV && node replace-assets-url.js",
    "assets:stg": "export ENV=STG && node replace-assets-url.js",
    "assets:prod": "export ENV=PROD && node replace-assets-url.js",
    "build:dev": "npm run assets:dev && ng build --configuration dev",
    "build:stg": "npm run assets:stg && ng build --configuration stg",
    "build:prod": "npm run assets:prod && ng build --configuration prod",
    "watch": "ng build --watch --configuration development",
    "test": "npm run assets && ng test --watch false",
    "coverage": "npm run assets && ng test --code-coverage --watch false",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "preinstall": "npx npm-force-resolutions",
    "lint": "ng lint",
    "prettier:list": "prettier --list-different './{libs,src}/**/*.ts' || ECHO .",
    "prettier:write": "prettier --write './{libs,src}/**/*.ts'",
    "prettier": "prettier --check './{libs,src}/**/*.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^18.0.6",
    "@angular-architects/module-federation-tools": "^19.0.3",
    "@angular/animations": "19.2.14",
    "@angular/common": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@ionic/angular": "^6.7.5",
    "@ionic/core": "^6.7.5",
    "@ngrx/effects": "^19.2.1",
    "@ngrx/router-store": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "@npm-bbta/bbog-dig-dt-webcomponents-lib": "^5.2.6",
    "crypto-js": "^4.2.0",
    "pkg-dir": "^8.0.0",
    "rxjs": "~7.5.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.0.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.7",
    "@angular-eslint/builder": "19.6.0",
    "@angular-eslint/eslint-plugin": "19.6.0",
    "@angular-eslint/eslint-plugin-template": "19.6.0",
    "@angular-eslint/schematics": "19.6.0",
    "@angular-eslint/template-parser": "19.6.0",
    "@angular/cli": "^19.0.7",
    "@angular/compiler": "^19.2.14",
    "@angular/compiler-cli": "^19.2.14",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~4.0.0",
    "@types/node-forge": "^1.3.11",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "ngx-build-plus": "^19.0.0",
    "raw-loader": "^4.0.2",
    "sass-alias": "^2.0.1",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1"
  }
}
